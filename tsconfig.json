{
  "compilerOptions": {
    "sourceMap": true,
    "outDir": "dist",
    "strict": true,
    "lib": [
      "esnext"
    ],
    "esModuleInterop": true,
    "skipLibCheck": true,
    "noEmitOnError": true,
    "suppressImplicitAnyIndexErrors": true,
    "plugins": [
      {
        "transform": "ts-transformer-keys/transformer"
      }
    ]
  },
  "files": [
    "index.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}